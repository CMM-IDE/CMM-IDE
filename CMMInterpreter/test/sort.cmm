int partition(int a[], int s, int t) {
        int i = s, j = t, temp = a[i];
        while (i < j) {
            while (j > i && a[j] >= temp)
                {j=j-1;}
            a[i] = a[j];
            while (i < j && a[i] <= temp)
               { i=i+1;}
            a[j] = a[i];
        }
        a[i] = temp;
        return i;
}

int sort(int a[], int low, int high) {
        if (low < high) {
            int i = partition(a, low, high);
            sort(a, 0, i - 1);
            sort(a, i + 1, high);
        }
        return 0;
}

int quickSort(int a[],int length) {
        sort(a, 0, length - 1);
        return 0;
}
